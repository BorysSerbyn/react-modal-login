{
  "name": "react-modal-login",
  "version": "1.0.6",
  "description": "A highly customizable react login modal with social buttons",
  "main": "dist/react-modal-login.js",
  "dependencies": {
    "classnames": "^2.1.3",
    "object-assign": "^4.1.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-proxy": "^1.1.8",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "reactify": "^1.1.1",
    "webpack": "^3.8.1",
    "less": "^2.7.2",
    "less-plugin-clean-css": "^1.5.1",
    "less-watch-compiler": "^1.8.0",
    "concurrently": "^3.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-watch": "^2.0.6",
    "babelify": "^7.3.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-latest": "^6.16.0",
    "browserify": "^14.4.0",
    "browserify-shim": "^3.8.12",
    "envify": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "livereactload": "^3.3.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "uglify-js": "^3.0.18",
    "uglify-js-es6": "^2.8.9",
    "uglifyify": "^4.0.1",
    "watchify": "^3.9.0"
  },
  "scripts": {
    "watch-css": "less-watch-compiler src/less demo style.less",
    "build-prod-js": "webpack",
    "build-css": "lessc --clean-css=\"--s1 --advanced --compatibility=ie8\" src/less/style.less dist/react-modal-login.min.css",
    "build-prod": "npm-run-all -s build-css build-prod-js",
    "build-dev": "node_modules/.bin/watchify --poll=500 demo/es/index.js -v -t babelify -p livereactload -o demo/bundle-compiled.js",
    "start": "concurrently -k \"npm run build-dev\" \"npm run watch-css\""
  },
  "author": "The Beaverhead",
  "person": {
    "name": "Olaf Meller",
    "email": "meller.olaf@gmail.com"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": true
        }
      ],
      "envify"
    ]
  }
}
